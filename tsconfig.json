{
  "compilerOptions": {
    "noEmit": true,
    "module": "ESNext",
    "target": "esnext",
    "lib": ["esnext"],
    "allowJs": true,
    "alwaysStrict": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "types": [
      "@types/react"
    ]
  }
}
